{
  "apiVersion": "v1",
  "description": "DMARC Report Parser - Monitor who's sending email on behalf of your domain",
  "name": "parse-dmarc",
  "spec": {
    "kind": "Deployment",
    "spec": {
      "billing": {
        "deploymentPlan": "nf-compute-10"
      },
      "deployment": {
        "buildConfiguration": {
          "pathIgnoreRules": [
            "**/node_modules",
            "**/.git"
          ]
        },
        "docker": {
          "configType": "default",
          "file": "/Dockerfile"
        }
      },
      "healthChecks": [
        {
          "failureThreshold": 3,
          "initialDelaySeconds": 10,
          "path": "/api/statistics",
          "periodSeconds": 30,
          "port": "web",
          "protocol": "HTTP",
          "successThreshold": 1,
          "timeoutSeconds": 5,
          "type": "HTTP"
        }
      ],
      "ports": [
        {
          "domains": [],
          "internalPort": 8080,
          "name": "web",
          "protocol": "HTTP",
          "public": true
        }
      ],
      "runtimeEnvironment": {},
      "runtimeFiles": {}
    }
  }
}
